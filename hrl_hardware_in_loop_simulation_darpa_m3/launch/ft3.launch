<launch>
    <node pkg="force_torque" name="ft3" type="force_torque_relay" args="--name=ft3" respawn="true"/>
    <node pkg="force_torque" name="ft3_poller" type="force_torque_poller" args="--name=ft3 --path=/dev/robot/force_torque3" respawn="true"/>

    <node name="static_tf_ft3_broadcaster" pkg="tf"
        type="static_transform_publisher"
        args="0 0 0 0 0 0.25881905 0.96592583 world force_torque_ft3 50"
        respawn="false" output="screen"/>
</launch>

